{
  "name": "react-samples",
  "version": "1.0.0",
  "description": "",
  "main": "src/entry.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack",
    "build:prod": "NODE_ENV=prod webpack",
    "start:cold": "webpack-dev-server --content-base build/",
    "start:hot": "HOT=1 webpack-dev-server --content-base build/ --hot --inline",
    "start": "WEBPACK_PUBLIC_PATH=http://localhost:8080/ npm run start:hot",
    "test": "npm run test:lint && jest",
    "test:lint": "lint -t typescript './src/**/*.tsx' --ignore ''"
  },
  "author": "Patrick Hulce <patrick.hulce@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "loading-page": "^1.0.1",
    "lodash": "^4.17.2",
    "nukecss-webpack": "^1.4.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@patrickhulce/lint": "^2.0.0",
    "@types/lodash": "^4.14.92",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^4.4.0",
    "@types/react-router": "^3.0.0",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "v2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "fontmin-webpack": "^1.0.2",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^17.0.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.4.1",
    "redbox-react": "^1.3.3",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.13.1",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.2.0"
  },
  "config": {
    "tslint": {
      "rules": {
        "whitespace": false,
        "no-inferred-empty-object-type": false,
        "newline-per-chained-call": false,
        "only-arrow-functions": false,
        "no-import-side-effect": false,
        "no-unsafe-any": false,
        "no-any": false
      }
    }
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(css|less|gif|ttf|eot|svg|woff)$": "<rootDir>/__mocks__/file-mock.js"
    }
  }
}
