{
  "name": "react-samples",
  "version": "1.0.0",
  "description": "",
  "main": "src/entry.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack",
    "build:prod": "NODE_ENV=prod webpack",
    "lint": "lint -t react --fix",
    "start:cold": "webpack-dev-server --content-base build/",
    "start:hot": "HOT=1 webpack-dev-server --content-base build/ --hot --inline",
    "start": "WEBPACK_PUBLIC_PATH=http://localhost:8080/ npm run start:hot",
    "test": "lint -t react && jest"
  },
  "author": "Patrick Hulce <patrick.hulce@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "font-awesome": "^4.7.0",
    "loading-page": "^1.0.1",
    "lodash": "^4.17.2",
    "nukecss-webpack": "^1.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@patritech/lint": "^1.2.2",
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "v2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "fontmin-webpack": "^1.0.2",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^17.0.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.4.1",
    "redbox-react": "^1.3.3",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.2.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(css|less|gif|ttf|eot|svg|woff)$": "<rootDir>/__mocks__/file-mock.js"
    }
  },
  "eslint": {
    "globals": [
      "__DEV__",
      "__PROD__",
      "__HOT__"
    ]
  }
}
